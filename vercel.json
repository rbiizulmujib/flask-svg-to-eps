{
    "version": 3,
    "builds": [
        {
            "src": "api-app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api-app.py"
        }
    ]
}